style.
  
  /* RESET */ 
  
  body {
    width: 100%;
    height: 100%;
    min-width: 100%;
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%;
    margin: 0;
    padding: 0;
  }
  
  table {
    border-spacing: 0;
  }
  
  td, 
  th {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    font-weight: normal;
    word-wrap: break-word;
    /* -webkit-hyphens: auto;
    -moz-hyphens: auto;
     hyphens: auto; */
  }
  
  table, 
  td {
    border-collapse:collapse;
  }

  img {
    border:0 none;
    height: auto;
    max-width: 100%;
    line-height:100%;
    outline:none;
    text-decoration:none;
    display: block;
  }

  a img {
    border:0 none;
  }
  
  
  /* CLIENT-SPECIFIC */
  
  /* Samsung Mail */
  
    /* Blue Links */
    #MessageViewBody a {
    color: inherit !important;
    text-decoration: none !important;
    font-size: inherit !important;
    font-family: inherit !important;
    font-weight: inherit !important;
    line-height: inherit !important;
    }
    

  /* Outlook.com / Hotmail */
  
    /* .External Class Override */
    .ExternalClass {
        width:100% !important;
    }

    .ExternalClass * {
        line-height: 100% !important;
    }


  /* Outlook */
  
    /* Visited Link Color */
    span.MsoHyperlink {
      mso-style-priority:99 !important;
      color:inherit !important;
    }
    span.MsoHyperlinkFollowed {
      mso-style-priority:99 !important;
      color:inherit !important;
    }
  
    /* "Read in Browser" Link */
    #outlook a {
      padding:0 !important;
    }
  
    /* table td Element Spacing */
    table, 
    td {
      mso-table-lspace:0pt !important;
      mso-table-rspace:0pt !important;
    }
  
    /* Image Resizing */
    img {
      -ms-interpolation-mode:bicubic !important;
    }
  
  /* OSX / iOS */
  
    /* WebKit Text Size Adjustment */
    body, 
    table, 
    td, 
    a {
      -webkit-text-size-adjust:100% !important;
    }
    
    /* iOS Blue Links */
    a[x-apple-data-detectors] {
      color: inherit !important;
      text-decoration: none !important;
      font-size: inherit !important;
      font-family: inherit !important;
      font-weight: inherit !important;
      line-height: inherit !important;
    }
    .appleFooter a {
      color:#99999 !important;
      text-decoration: none !important;
    }
    
    /* Legacy iOS Blue Link */
    .appleLinks,
    .appleLinks a {
      color: inherit !important;
      text-decoration: none !important;
    }
    
  /* Android */
  
    /* Center Fix */
    div[style*="margin: 16px 0;"] {
    margin: 0 !important;
    }
     
  /* Windows Mobile */
    
   /* Windows Mobile Text Size Adjustment */
    body, 
    table, 
    td,
    a {
      -ms-text-size-adjust:100% !important;
    }
    
    
  /* VIDEO */
    .vid {
      display: none !important;
    }

    @media (-webkit-min-device-pixel-ratio: 0) and (min-device-width:1024px) {
      .vid {
        display: block !important;
      }

      .vid-fallback {
        display: none !important;
      }
    }

    /* iOS 10+ Video Support */
    @supports (-webkit-overflow-scrolling:touch) and (color:#ffffff) {
      div[class^=vid] {
        display: block !important;
      }

      div[class^=vid-fallback] {
        display: none !important;
      }
    }

    /* Samsung Video Support */
    #MessageViewBody .vid {
      display: block !important;
    }

    #MessageViewBody .vid-fallback {
      display: none !important;
    }
  

  /* CUSTOM */ 

  @media only screen and (max-width: 600px) {
    .hide {
      display: none !important;
    }
    
    img {
      max-width:100% !important;
    }
    
    video {
      width: 100% !important;
    }
    
